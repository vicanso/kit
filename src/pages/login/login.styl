@import '../../styles/common'

header
  height $MAIN_HEADER_HEIGHT 
  line-height @height
  background-color $COLOR_BLUE
  color $COLOR_WHITE
  position relative
  a
    color $COLOR_WHITE
  .back
    width $MAIN_HEADER_HEIGHT
    position absolute
    left 0
    height 100%
    font-size 28px
form
  margin 15px 30px
  input
    width 100%
    margin-bottom 15px !important
  button
    border none
    width 100%
    line-height 2